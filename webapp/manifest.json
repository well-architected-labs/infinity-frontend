					"scopes": [
						"*"
					]
				},
				{
					"pattern": "sign-in",
					"name": "SignIn",
					"target": "SignIn",
					"scopes": [
						"*"
					]
				},
				{
					"pattern": "terms",
					"name": "Legal",
