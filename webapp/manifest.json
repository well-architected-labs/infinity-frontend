{
	"_version": "1.0.0",
	"sap.app": {
		"id": "com.erp.myapp",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "com.erp.myapp.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.115.1",
			"libs": {
				"sap.ui.core": {},
				"sap.tnt": {},
				"sap.m": {},
				"sap.ui.layout": {}
			}
		},
		"handleValidation": true,
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.erp.myapp.i18n.i18n"
				}
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "com.erp.myapp.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"async": true
			},
			"routes": [
				{
					"pattern": "",
					"name": "main",
					"target": "main",
					"scopes": [
						"*"
					]
				},
				{
					"pattern": "home",
					"name": "home",
					"target": "home",
					"scopes": [
						"administrator:person:vacancy:read",
						"administrator:vacancy:editor"
					]
				},
				{
					"pattern": "notFound",
					"name": "notFound",
					"target": "notFound",
					"scopes": [
						"*"
					]
				},
				{
					"pattern": "logout",
					"name": "logout",
					"target": "logout",
					"scopes": [
						"administrator:person:vacancy:read",
						"administrator:vacancy:editor"
					]
				},
				{
					"pattern": "vacancies",
					"name": "vacancies",
					"target": "vacancies",
					"scopes": [
						"administrator:company:vacancy:editor",
						"administrator:vacancy:read",
						"administrator:vacancy:editor"
					]
				},
				{
					"pattern": "vacancies/candidated",
					"name": "VacanciesList",
					"target": "VacanciesList",
					"scopes": [
						"administrator:person:vacancy:view",
						"administrator:company:vacancy:editor"
					]
				},
				{
					"pattern": "vacancies/edit/:id:",
					"name": "VacanciesEdit",
					"target": "VacanciesEdit",
					"scopes": [
						"administrator:company:vacancy:editor",
						"administrator:vacancy:editor"
					]
				},
				{
					"pattern": "vacancies/view/:id:",
					"name": "VacanciesView",
					"target": "VacanciesView",
